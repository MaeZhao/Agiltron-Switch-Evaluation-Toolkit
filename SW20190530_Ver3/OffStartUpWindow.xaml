<Window x:Class="SW20190530_Ver3.OffStartUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SW20190530_Ver3"
        mc:Ignorable="d"
        Title="Offline Start Up" Height="110" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock FontSize="12" Grid.Row="0" HorizontalAlignment="Center" Margin="0,10,0,15" >The program is running in offline mode.</TextBlock>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Button Name = "back" Grid.Column="0" HorizontalAlignment="Right" Width="50" Margin="0,0,20,0">Cancle</Button>
            <Button Name = "cont" Grid.Column="1" HorizontalAlignment="Left" Width="50" Margin="20,0,0,0">OK</Button>

        </Grid>
    </Grid>
</Window>
